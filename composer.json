{
    "name": "toolstation/omnipay-worldpay-redirect",
    "type": "library",
    "description": "WorldPay Redirect XML driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "worldpay",
        "payment",
        "redirect"
    ],
    "homepage": "https://github.com/toolstation/omnipay-worldpay-redirect",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Snellock",
            "email": "nick.snellock@toolstation.com"
        }
    ],
    "autoload": {
        "psr-0": { "Omnipay\\WorldPayRedirect\\" : "src/" }
    },
    "require": {
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "scripts": {
        "test": "phpunit tests/",
        "cs:check": "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.php --dry-run",
        "cs:fix": "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.php"
    }
}
